<form class="new-user" action="<%= users_url %>" method="Post">
	<%= auth_token %>
	<label for="user_email">Email</label>
	<input type="text"
				 name="user[email]"
				 id="user_email"
				 value="<%= @user.email %>">

	<label for="user_password">Password</label>
	<input type="password"
				 name="user[password]"
				 id="user_password">

	<input type="submit" value="Create Account">
</form>